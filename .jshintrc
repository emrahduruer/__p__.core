{
    "node": true,
    "browser": true,
    "esnext": true,
    "bitwise": true,
    "camelcase": false,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 4,
    "latedef": false,
    "newcap": true,
    "noarg": true,
    "quotmark": "single",
    "regexp": true,
    "undef": true,
    "unused": true,
    "strict": false,
    "trailing": true,
    "smarttabs": true,
    "predef": [
        "$",
        "jQuery",
    	"_",
        "describe",
        "it",
        "expect",
        "toBe",
        "beforeEach",
        "spyOn",
        "setFixtures"
    ]
}
